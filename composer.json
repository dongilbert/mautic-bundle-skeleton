{
  "name": "friendsofmautic/bundle-skeleton",
  "license": "MIT",
  "type": "library",
  "description": "Mautic files available via composer for easier develop of a new bundle",
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/mautic/mautic.git"
    }
  ],
  "require": {
    "php": ">=5.4",
    "mautic/core": "1.3.0"
  },
  "require-dev": {
    "php": ">=7.0",
    "phpunit/phpunit": "~6.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
